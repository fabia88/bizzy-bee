<div class="container">
  <div class="row">
    <div class="col-xs-6-offset-3">
      <div class="form-group">
        <%= simple_form_for([@job, @request]) do |f| %>
        <%= f.input :date, class: "form-control" %>
        <%= f.input :duration, class: "form-control" %>
        <%= f.submit %>
        <% end %>
      </div>
    </div>
  </div>
</div>
